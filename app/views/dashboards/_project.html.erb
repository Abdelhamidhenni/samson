<tr class="<%= 'warning' if deploy_group_versions.values.uniq.count > 1 %>">
  <td><%= link_to project.name, project %></td>
  <% @environment.deploy_groups.each do |deploy_group| %>
    <td>
      <%= link_to deploy_group_versions[deploy_group.id].reference, project_deploy_path(project, deploy_group_versions[deploy_group.id]) if deploy_group_versions[deploy_group.id] %>
    </td>
  <% end %>
</tr>
